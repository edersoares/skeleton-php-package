{
    "name": "dex/skeleton-php-package",
    "description": "Skeleton for PHP packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eder Soares",
            "email": "edersoares@me.com",
            "homepage": "https://edersoares.me",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dex\\Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dex\\Skeleton\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit"
    },
    "scripts-descriptions": {
        "tests": "Run tests"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
